#
# ~/.bash_aliases
#

# Aliases
alias l='ls -Falh'
alias fetch='neofetch'

# dotfile
alias dot='/usr/bin/git --git-dir=$HOME/github/dotfiles/ --work-tree=$HOME'
alias dota='dot add'
alias dots='dot status'
alias dotc='dot commit -m'

# git
alias ga='git add'
alias gc='git commit -m'
alias gs='git status'
